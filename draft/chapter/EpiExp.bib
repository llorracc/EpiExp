@Article{	  branchheteroexp,
  author	= {Branch, William A.},
  title		= "{The Theory of Rationally Heterogeneous Expectations:
		  Evidence from Survey Data on Inflation Expectations}",
  journal	= {The Economic Journal},
  volume	= {114},
  number	= {497},
  pages		= {592-621},
  year		= {2004},
  month		= {06},
  abstract	= "{Previous work with survey data on inflationary
		  expectations casts doubt on the Rational Expectations
		  Hypothesis. In this paper, we develop a model of
		  expectation formation where agents form their forecasts of
		  inflation by selecting a predictor function from a set of
		  costly alternatives whereby they may rationally choose a
		  method other than the most accurate. We use this model to
		  test whether survey data exhibit rationally heterogeneous
		  expectations. Maximum likelihood is applied to a new
		  discrete choice setting where the observed variable is
		  continuous and the latent variable is discrete. The results
		  show there is dynamic switching that depends on the
		  relative mean squared errors of the predictors.}",
  issn		= {0013-0133},
  doi		= {10.1111/j.1468-0297.2004.00233.x},
  url		= {https://doi.org/10.1111/j.1468-0297.2004.00233.x},
  eprint	= {https://academic.oup.com/ej/article-pdf/114/497/592/26449073/ej0592.pdf}
}

